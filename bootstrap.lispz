(using [net github]
  (lispz.css "ext/bootstrap.css")
  (lispz.script "ext/bootstrap.js" (=>
    (export {
      build: (lambda [target-repo built=>]
        (github.build target-repo "bootstrap" [[
          {repo: "twbs/bootstrap" files: [[
            {base: "dist" exclude: '/\.map$|\.min\.|npm.js$/'}
            {base: "dist/fonts" copy-to: "fonts"}
          ]]}
        ]] built=>)
      )
    })
  ))
)
