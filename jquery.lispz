(using  "net"
  /*** Load jquery from the CDN and continue normally once all is done ***/
  (observe
    (net.cdnjs-latest "jquery")
    (on-error [error] (alert (+ "CDNJS ERROR loading jquery\n" error)))
    (then [uri]
      (net.script uri)
      (on-error [error] (alert (+ "CDNJS ERROR loading jquery\n" error)))
      (then [] (export {jquery: $})))
  )
)
